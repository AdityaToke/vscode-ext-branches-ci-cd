{
	"name": "git-branches-ci-cd",
	"displayName": "Git: Branch CI/CD",
	"publisher": "AdityaToke",
	"description": "",
	"version": "0.0.1",
	"repository": {
		"url": "https://github.com/AdityaToke/vscode-ext-branches-ci-cd"
	},
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:git-branches-ci-cd.initBranchTask"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "git-branches-ci-cd.initBranchTask",
				"title": "Git: Branch CI/CD"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"pack2": "yarn vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"vsce": "^1.96.1"
	}
}
